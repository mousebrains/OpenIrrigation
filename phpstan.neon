parameters:
  level: 6
  paths:
    - public_html
  excludePaths:
    - public_html/packages
    - public_html/js
    - public_html/css
  reportUnmatchedIgnoredErrors: false
  ignoreErrors:
    # config.php is generated at build time via ./config; not in the repo
    - identifier: requireOnce.fileNotFound
    # OI_DBNAME is defined in the generated config.php
    - identifier: constant.notFound
